{
  "name": "Veneer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Use your own CSS across the web!",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
    },
    "content_scripts": [{
      "matches": [
      "http://*/*",
      "https://*/*"
      ],
      "js": ["scripts/content.js"],
      "run_at": "document_end"
    }],
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {
      "scripts": ["scripts/background.js"]
    },
    "permissions": [
      "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking"
  ],
  "default_locale": "en"
}
