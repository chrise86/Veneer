{
  "name": "Font Smooth",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Smooth fonts like Safari",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
    },
    "content_scripts": [{
      "matches": [
      "http://*/*",
      "https://*/*"
      ],
      "js": ["scripts/content.js"],
      "run_at": "document_end"
    }],
  "default_locale": "en"
}
